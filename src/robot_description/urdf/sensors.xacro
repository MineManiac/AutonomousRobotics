<!-- IMU Plugin -->
<gazebo reference="imu_link">
    <sensor name="my_imu" type="imu">
        <always_on>true</always_on>
        <update_rate>30</update_rate>
        <plugin name="imu_plugin" filename="libgazebo_ros_imu_sensor.so">
            <ros>
                <namespace>/</namespace>
                <!--<remapping></remapping> -->
            </ros>
        </plugin>
    </sensor>
</gazebo>